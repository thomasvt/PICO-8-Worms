-- camera : c
cam_x=0
cam_y=0
cam_x_internal=0

function c_update()
 if btn(➡️) then cam_x_internal+=1 end
 if btn(⬅️) then cam_x_internal-=1 end
 if btn(⬇️) then cam_y+=1 end
 if btn(⬆️) then cam_y-=1 end

 -- round to even (for pixel memory)
	cam_x = cam_x_internal \ 2 * 2
end
