-- camera : c
cam_x=0
cam_y=0
cam_x_int=0
c_zoom = 1
c_zoomed = false

cam_speed_x = 8

c_chase = false

function c_update()
 -- follow target
 local target = w_p.x-64
 local extra = w_aiming and 45 or 10
	target += (w_p.look < 0) and -extra or extra
 
 local dist = abs(target-cam_x_int) 
 
 if c_chase or (w_p.thrst == 0 and w_p.grnd) then
  if target > cam_x_int then
   cam_x_int += min(cam_speed_x,dist)
  else
   cam_x_int -= min(cam_speed_x,dist)
  end
  if dist < 2 then
   c_chase = false
  end
 else
  if dist > 40 then
   c_chase = true
  end
 end
 
 -- vertical:
 target = w_p.y-64
 dist = abs(target - cam_y)
 if dist > 10 then
  if cam_y < target then
   cam_y += target-cam_y-10
  else
   cam_y -= cam_y-target-10
  end
 end

 -- round to even (for pixel memory)
	cam_x = cam_x_int \ 2 * 2
end

function c_toggle_zoom()
 c_zoomed = not c_zoomed
	c_zoom = c_zoomed and 2 or 1
	sprite_shift = c_zoomed and 8 or 0	 
end

-- world 2 scr coords
-- for any { x,y }
function c_wrld_to_scr(v)
 if c_zoomed then
  return {
   x = v.x\2,
   y = v.y\2 + 31
  }
 else
  return {
   x = v.x - cam_x,
   y = v.y - cam_y
  }
 end
end
