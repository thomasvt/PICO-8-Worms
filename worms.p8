-- worms : w

worms = {}
add(worms, {
	x=40,
	y=20,
	vx=0,
	vy=0,
	dx=0, -- accumul delta x (move in integers)
	dy=0,
	walk=0,
	flip=false, -- flip sprite horiz?
	spr=1, -- sprite idx
	grnd=false -- on ground?
	})
w_cur = worms[1]

function w_update()
 player_ctrl() 
 foreach(worms, w_upd_worm)
end

function player_ctrl()
 -- jump?
 if w_cur.grnd and btn(4) then 
  w_cur.vy = -2 
  if w_cur.flip then
   w_cur.vx = -0.8
  else
   w_cur.vx = 0.8
  end
  w_cur.grnd = false
 end
 
 -- walk?
 w_cur.walk = 0
 if w_cur.vy == 0 and (btn(➡️) or btn(⬅️)) then
  w_cur.walk = 0.3 -- walk speed
  if btn(⬅️) then w_cur.walk *= -1 end
 end
end

function w_upd_worm(w)	
 w.grnd = l_obstacle(w.x, w.y+5)

 -- push out of ground (horiz slope)
	while l_obstacle(w.x, w.y+4) or l_obstacle(w.x, w.y+3) do
	 w.y -= 1
	end

 -- look-direction
 if w_cur.walk != 0 then
  w_cur.flip = w_cur.walk < 0
 end

 -- which sprite?
 
 if w_cur.walk != 0 then
  w_cur.spr = 3 + frame\10 % 2
 else
  w_cur.spr = 1
  if w_cur.vy < 0 then 
  	w_cur.spr = 5 
  end
  if w_cur.vy > 1 then
   w_cur.spr = 6 
  end
 end

 if not w.grnd then
 	w.vy += 0.15 -- gravity
 else
  w.vx \= 2
	end
	
	-- movement
	w.dx += w.vx + w.walk
	w.dy += w.vy
		
	-- ➡️
	while w.dx>1 do
	 if l_obstacle(w.x+1, w.y) then
	   -- collide wall
    w.dx = 0
 	  w.vx = 0
		 else 
		  w.x += 1
		  w.dx -= 1
		 end
	end
	
	-- ⬅️
	while w.dx<-1 do
	 if l_obstacle(w.x-1, w.y) then
	   -- collide wall
    w.dx = 0
 	  w.vx = 0
		 else 
		  w.x -= 1
		  w.dx += 1
		 end
	end
	
	-- ⬇️
	while w.dy>1 do
	 if l_obstacle(w.x, w.y+5) then
	   -- collide floor
    w.dy = 0
 	  w.vy = 0
 	  w.vx = 0
		 else 
		  w.y += 1
		  w.dy -= 1
		 end
	end
	
	-- ⬆️
	while w.dy<-1 do
	 if l_obstacle(w.x, w.y-5) then
	   -- collide ceiling
    w.dy = 0
 	  w.vy = 0
		 else 
		  w.y -= 1
		  w.dy += 1
		 end
	end
	
end

function w_draw() 
	foreach(worms, w_drw_worm)
end

function w_drw_worm(w)
 spr(w.spr,
  w.x-3-cam_x,
  w.y-3-cam_y,
  1,1,w.flip,false
  )
end
