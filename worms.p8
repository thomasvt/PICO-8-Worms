-- players/worms : p 

worms = {}
add(worms, {
	x=40,
	y=20,
	vx=0,
	vy=0,
	dx=0, -- accumul delta x (move in integers)
	dy=0,
	flip=false
	})
cur_worm = worms[1]

function p_update()
 -- jump?
 if btn(4) then cur_worm.vy = -2 end
 -- move horiz?
 cur_worm.vx = 0
 if btn(➡️) then 
  cur_worm.flip = false 
  cur_worm.vx = 1
 end
 if btn(⬅️) then 
  cur_worm.flip = true 
  cur_worm.vx = -1
 end
 
 foreach(worms, p_upd_worm)
end

function p_upd_worm(w)	
	w.vy += 0.15 -- gravity
	
	w.dx += w.vx
	w.dy += w.dy
	
	local dxi = w.vx\1
	
	-- move right
	if dxi>0 then
	 for i = 1,dxi do
		 if not w_obstacle(w.x+1, w.y) then
		  w.x += 1
		  w.dx -= 1
		 else -- collide wall
 	  w.vx = 0
    break
		 end
	 end
	end
	
	-- move left
	if dxi<0 then
	 for i = -1,dxi,-1 do
		 if not w_obstacle(w.x-1, w.y) then
		  w.x -= 1
		  w.dx += 1
		 else  -- collide wall
		  w.vx = 0
		  break
		 end
	 end
	end
	
	
	local dy = w.vy\1
	
	-- fall	
	
	if dy>0 then	 
	 for i = 1,dy do
		 if not w_obstacle(w.x, w.y+5) then
		  w.y += 1
		 else -- land on ground
		  w.vy = 0
 	  w.vx = 0
	   break
		 end
	 end
	end
	
 -- rise
	if dy<0 then
	 for i = -1,dy,-1 do
		 if not w_obstacle(w.x, w.y-5) then
		  w.y -= 1
		 else  -- collide ceiling
		  w.vy = 0
		  break
		 end
	 end
	end
end

function p_draw() 
	foreach(worms, p_drw_worm)
end

function p_drw_worm(w)
 spr(1,
  w.x-3-cam_x,
  w.y-3-cam_y,
  1,1,w.flip,false
  )
end
