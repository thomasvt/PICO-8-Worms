bodies = {}
p_lastmove = time()

function p_integrate(b)
 local last_x=b.x
 local last_y=b.y

 b.collide = false

 if b.y >= 200 or b.x < -100 or b.x > 356 then 
 
  b.collide = true 
  b.collide_force = 10000 
  b.out_of_arena = true
  return
 end

 -- on ground?
 b.grnd = l_obstacle(b.x, b.y+b.r+1, b)

 -- push out of ground (horiz slope)
	while l_obstacle(b.x, b.y+b.r, b) 
	   or l_obstacle(b.x, b.y+b.r-1, b) do
	 b.y -= 1
	end
	
	-- gravity
 if b.grnd then
  b.vx /= 2 	
 else
  b.vy += 0.15
	end

 -- accumulate distance to move
 b.dx += b.vx + b.thrst
	b.dy += b.vy
		
	-- ‚û°Ô∏è
	while b.dx>=1 do
	 if l_obstacle(b.x+1, b.y, b) then
 	  b.collide_force = b.vx
    b.dx = 0
 	  b.vx = 0
 	  b.collide = true
		 else 
		  b.x += 1
		  b.dx -= 1
		 end
	end
	
	-- ‚¨ÖÔ∏è
	while b.dx<=-1 do
	 if l_obstacle(b.x-1, b.y, b) then
 	  b.collide_force = b.vx
    b.dx = 0
 	  b.vx = 0
 	  b.collide = true
		 else 
		  b.x -= 1
		  b.dx += 1
		 end
	end
	
	-- ‚¨áÔ∏è
	while b.dy>=1 do
	 if l_obstacle(b.x, b.y+b.r+1, b) then
 	  b.collide_force = b.vy
    b.dy = 0
 	  b.vy = 0
 	  b.vx = 0
 	  b.collide = true
		 else 
		  b.y += 1
		  b.dy -= 1
		 end
	end
	
	-- ‚¨ÜÔ∏è
	while b.dy<=-1 do
	 if l_obstacle(b.x, b.y-b.r, b) then
 	  b.collide_force = b.vy
    b.dy = 0
 	  b.vy = 0
 	  b.vx /= 2
 	  b.collide = true
		 else 
		  b.y -= 1
		  b.dy += 1
		 end
	end
	
	if last_x != b.x or last_y != b.y then
	 p_lastmove = time()
	end
end

function p_draw(b)
  local v = c_wrld_to_scr(b)
  spr(b.spr+sprite_shift,
   v.x-3, v.y-3,
   1,1,
   b.look<0,
   false
   )
end
