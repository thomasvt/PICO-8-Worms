-- main loop

frame = 0

function _init()
	l_gen()
end

function _update()
 w_update()
 c_update()
 
 frame+=1
 if frame==1024 then frame = 0 end

end

function _draw()
	cls(0)
	l_draw()
	w_draw()
 print(stat(1))	
end
