-- worms : w

worms = {}
add(worms, {
	x=40,
	y=20,
	vx=0,
	vy=0,
	dx=0, -- accumul delta x (move in integers)
	dy=0,
	thrst=0, -- thrust x (walk)
	look=1, -- 1=right,-1=left
	spr=1, -- sprite idx
	grnd=false -- on ground?
	})
w_cur = worms[1]

function w_update()
 player_ctrl() 
 foreach(worms, w_upd_worm)
end

function player_ctrl()
 -- jump?
 if w_cur.grnd and btn(4) then 
  w_cur.vy = -1.5 
  w_cur.vx = w_cur.look
  w_cur.grnd = false
 end
 
 -- walk?
 w_cur.thrst = 0
 if w_cur.vy == 0 and (btn(➡️) or btn(⬅️)) then
  w_cur.thrst = 0.3 -- walk speed
  if btn(⬅️) then w_cur.thrst *= -1 end
 end
end

function w_upd_worm(w)	

 -- look-direction
 if w_cur.thrst > 0 then
  w_cur.look = 1
 end
 if w_cur.thrst < 0 then
  w_cur.look = -1
 end

 -- which sprite?
 
 if w_cur.thrst != 0 then
  w_cur.spr = 3 + frame\10 % 2
 else
  w_cur.spr = 1
  if w_cur.vy < 0 then 
  	w_cur.spr = 5 
  end
  if w_cur.vy > 1 then
   w_cur.spr = 6 
  end
 end

 p_integrate(w)	
end

function w_draw() 
	foreach(worms, w_drw_worm)
end

function w_drw_worm(w)
 spr(w.spr,
  w.x-3-cam_x,
  w.y-3-cam_y,
  1,1,w.look<0,false
  )
end
