-- bullets b

b_dir2spr = {0,1,2,1,0,3,4,3}

bullets = {}

	
function b_fire(x,y,vx,vy)
 local b = m_newobj(x,y,vx,vy,16,2)
	add(bodies, b)
	add(bullets, b)
end
	
function b_update(b)
	-- collide?
	if b.collide then
  del(bodies,b)
  del(bullets,b)
  b_explode(b.x, b.y)
	end

 -- set sprite 4 direction:
 if b.vx != 0 then b.look = b.vx end
 
 if b.vx != 0 or b.vy != 0 then
	 local x = abs(b.vx)
	 local a = ((atan2(x,b.vy)*8+0.5)\1)%8 
		b.spr=b_dir2spr[a+1]+16	 		
	end
end

function b_explode(x,y)
 local r = 10
 local hr = r/2
 
 -- small smoke parts:
 for i=1,8 do
  local a = rnd(32)/32
  r_emit(
   x+cos(a)*(hr+rnd(hr)), 
   y+sin(a)*(hr+rnd(hr)),
   -0.5, -- vy
   2+rnd(hr),-0.1, --radius 
   20+rnd(40), -- lifetime
   5+rnd(3)) -- color
 end
 
 -- big flash:
 r_emit(x, y,0, r*2,-3, 4,9)
 l_destroy(x,y, r)
 
 b_hit_bodies(x,y,r*3)
end

function b_hit_bodies(x,y,r)
 y += 5 // lower epicenter=more upwards throwing
 
 for b in all(bodies) do
  if is_in_range(b.x,b.y, x,y, r) then
   local d = dist(b.x,b.y, x,y)
   local force_pct = 
     1/d -- normalize 
     * (r-d)/r -- [0..1]
   
   b.vx = (b.x - x) * force_pct*7
   b.vy = (b.y - y) * force_pct*7

   -- damage it
   if b.health then
    w_hurt(b, min(50, force_pct*1000))
   end
  end
 end
end
