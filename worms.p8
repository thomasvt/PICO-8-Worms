-- camera : c
cam_x=0
cam_y=0
cam_x_int=0

c_chase = false

function c_update()
 local target = w_cur.x-64
 local dist = abs(target-cam_x_int) 
 
 if c_chase or (w_cur.thrst == 0 and w_cur.grnd) then
  if target > cam_x_int then
   cam_x_int += min(3,dist)
  else
   cam_x_int -= min(3,dist)
  end
  if dist < 2 then
   c_chase = false
  end
 else
  if dist > 30 then
   c_chase = true
  end
 end
 
 cam_y = w_cur.y-64

 -- round to even (for pixel memory)
	cam_x = cam_x_int \ 2 * 2
end
