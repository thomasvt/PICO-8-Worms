-- physics : p

function p_integrate(o)
 -- on ground?
 o.grnd = l_obstacle(o.x, o.y+5)

 -- push out of ground (horiz slope)
	while l_obstacle(o.x, o.y+4) 
	   or l_obstacle(o.x, o.y+3) do
	 o.y -= 1
	end
	
	-- gravity
 if o.grnd then
  o.vx \= 2 	
 else
  o.vy += 0.15
	end

 -- accumulate distance to move
 o.dx += o.vx + o.thrst
	o.dy += o.vy
		
	-- ➡️
	while o.dx>1 do
	 if l_obstacle(o.x+1, o.y) then
    o.dx = 0
 	  o.vx = 0
		 else 
		  o.x += 1
		  o.dx -= 1
		 end
	end
	
	-- ⬅️
	while o.dx<-1 do
	 if l_obstacle(o.x-1, o.y) then
    o.dx = 0
 	  o.vx = 0
		 else 
		  o.x -= 1
		  o.dx += 1
		 end
	end
	
	-- ⬇️
	while o.dy>1 do
	 if l_obstacle(o.x, o.y+5) then
    o.dy = 0
 	  o.vy = 0
 	  o.vx = 0
		 else 
		  o.y += 1
		  o.dy -= 1
		 end
	end
	
	-- ⬆️
	while o.dy<-1 do
	 if l_obstacle(o.x, o.y-4) then
    o.dy = 0
 	  o.vy = 0
 	  o.vx /= 2
		 else 
		  o.y -= 1
		  o.dy += 1
		 end
	end
end


